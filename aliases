# Unix
unalias rm
alias ll="ls -al"
alias ln="ln -v"
alias lsaa="ls -hA"
alias zprezto-update="cd ~/.zprezto && git pull --rebase && git submodule update --init --recursive && cd -"

# Pretty print the path
alias path='echo $PATH | tr -s ":" "\n"'

# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local

